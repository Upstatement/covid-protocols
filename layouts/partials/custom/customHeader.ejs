<div class="custom-header custom-header--has-sidebar">
  <span class="visually-hidden"><%- template('branding.org') %></span>
  <a href="/">
    <img src=<%- template('branding.logo') %> class="custom-header__logo"
    alt="<%- template('branding.org') %>">
  </a>

  <nav>
    <% if (locals.topNav && locals.topNav.length) { %> <%
    locals.topNav.forEach((item) => { %> <% if (item.prettyName === 'Homepage')
    { %>
    <a href="/">Home</a>
    <% } else { %>
    <a href="<%- item.path %>"><%- item.prettyName %></a>
    <% } %> <% }) %> <% } %>
  </nav>

  <form method="GET" action="/search" class="nav-search-container">
    <input
      id="nav-search-box"
      class="twitter-typeahead"
      type="text"
      name="q"
      value=""
      placeholder="Search..."
    />
    <button class="icon"><i class="fa fa-search"></i></button>
  </form>
</div>
