<!DOCTYPE html>
<html>
  <%- include ../partials/head %>
  <body>
    <h1 class="visually-hidden">Default template</h1>
  <main class="site-page">
    <%- include ../partials/header %> <%- include ../partials/nav %>
    <div class="g-body">
      <div class="g-main-content">
        <% if (locals.content) { %> <%- content %> <% } %> <% if
        (locals.children && children.length) { %> <%-
        include('../partials/childrenList', {children, kicker:
        template('folder.childrenList.kicker', title)}) %> <% } %>
      </div>

      <%- include('../partials/footer', { pageType: 'document', topLevelFolder:
      url.split('/')[1] }) %>
    </div>
  </main>
  </body>
</html>
